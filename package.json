{
  "name": "deeID",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "./config/postinstall.sh",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "test:watch": "jest --watch",
    "android:build": "cd android && ./gradlew assembleDebug && cd .. && cp android/app/build/outputs/apk/debug/app-debug.apk deeID.apk",
    "android:bundle": "react-native bundle --dev false --platform android --entry-file index.js --bundle-output ./android/app/build/intermediates/assets/debug/index.android.bundle --assets-dest ./android/app/build/intermediates/res/merged/debug",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "android:generate-apk": "npm run android:bundle && npm run android:build",
    "android": "react-native run-android",
    "logcat": "adb logcat *:S ReactNative:V ReactNativeJS:V",
    "ios": "react-native run-ios"
  },
  "dependencies": {
    "autobind-decorator": "2.1.0",
    "axios": "0.18.0",
    "ethers": "2.2.6",
    "identicon.js": "2.3.1",
    "jshint": "^2.9.6",
    "mobx": "3.6.1",
    "mobx-react": "4.4.2",
    "moment": "2.21.0",
    "native-base": "^2.12.1",
    "npm": "^6.4.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-native": "^0.59.4",
    "react-native-camera": "^1.5.1",
    "react-native-draggable-view": "0.0.6",
    "react-native-linear-gradient": "^2.5.4",
    "react-native-local-mongodb": "^2.2.3",
    "react-native-modal": "5.2.0",
    "react-native-permissions": "1.1.1",
    "react-native-qrcode-svg": "5.0.6",
    "react-native-sensitive-info": "5.1.0",
    "react-native-simple-dialogs": "^1.2.1",
    "react-native-snackbar": "0.4.6",
    "react-native-svg": "6.2.2",
    "react-native-vector-icons": "4.5.0",
    "react-navigation": "1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-flow": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-react-native": "^5.0.2",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.0",
    "eslint-config-rallycoding": "3.2.0",
    "jest": "22.4.2",
    "jsdom": "^16.5.0",
    "react-native-mock-render": "0.0.19",
    "react-test-renderer": "16.2.0",
    "sinon": "4.4.2"
  },
  "jest": {
    "preset": "react-native",
    "setupTestFrameworkScriptFile": "<rootDir>/config/jest.config.js"
  }
}
